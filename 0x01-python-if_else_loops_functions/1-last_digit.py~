#!/usr/bin/python3
import random
number = random.randint(-10000, 10000)
index_msg = "Last digit of"
if number < 0:
    last_num = abs(number) % 10
    last_num = (-last_num)
else:
    last_num = number % 10
if last_num > 5:
    print(f"{index_msg} {number} is {last_num} and is greater than 5")
elif last_num == 0:
    print(f"{index_msg} {number} is {last_num} and is 0")
elif last_num < 6 and last_num != 0:
    print(f"{prefix} {number} is {last_digit} and is less than 6 and not 0")
